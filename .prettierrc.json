{
    "parser": "acorn",
    "printWidth": 160,
    "tabWidth": 4,
    "trailingComma": "es5",
    "semi": true,
    "singleQuote": true,
    "bracketSpacing": true,
      "arrowParens": "always",
      "insertPragma": true,
      "htmlWhitespaceSensitivity": "strict",
      "endOfLine": "lf",
      "embeddedLanguageFormatting": "auto",
      "singleAttributePerLine": true,
      "overrides": [
          {
              "files": ".prettierrc",
              "options": {
                  "parser": "json"
              }
          }
      ]
}
